CONSTANTS #
$bui_UaBui    = 168     ! Heat loss rate, W/K
$bui_Vbui     = 389.45    ! air volume of the building [m3]
$bui_UGrFloor = 0.183   ! U-Value of Ground floor, W/m2K

CONSTANTS #
$bui_PheatBuiD    = 12830  ! design heating rate for building an location [kJ/h]
$bui_TBuiFlNom    = 35     ! Design flow temp. heating syst. [�C]
$bui_TBuiRtNom    = 30     ! Design return temp. heating syst. [�C]
$bui_AreaBui      = 120
$bui_TroomSetCool = 24
$bui_TroomSet     = 21.00     ! Setpoint for Room temperature, �C
$bui_m_Rd         = 1.3

CONSTANTS #
factorMfrBuiNom = 1 !adapt this factor to achieve the nominal DeltaT you need in the dustribution loop
$bui_QPeak_kW   = 5
$bui_MfrBuiNom  = factorMfrBuiNom*$bui_QPeak_kW/($cpWat*($bui_TBuiFlNom-$bui_TBuiRtNom))*3600 ! Nominal mass flow of heat distribution system [kg/h]

unitAssignBuilding = 56

ASSIGN PATH$\Loads_and_structures\Multi_Zone_Models\type56\database\buifiles\T44A38sfh045.bui unitAssignBuilding     ! Building def. file, -
