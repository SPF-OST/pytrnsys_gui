            <dimension>Dimensionless</dimension>
            <unit>-</unit>
            <type>integer</type>
            <min>1</min>
            <max>+Inf</max>
            <boundaries>[ ; ]</boundaries>
            <default>7</default>
            <symbol>SN</symbol>
            <definition>Number of data points for the IAM (transverse direction)</definition>
