        <variable><!--input-6-->
            <order>6</order>
            <name>Solar incidence angle</name>
            <role>input</role>
            <dimension>Direction (Angle)</dimension>
            <unit>degrees</unit>
            <type>real</type>
            <min>-360</min>
            <max>+360</max>
            <boundaries>[ ; ]</boundaries>
            <default>0.0</default>
            <symbol>SN</symbol>
